@model IEnumerable<SensorProjectV1._0.Models.Token>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TokenID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device.DeviceID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <th>
            @Html.DisplayFor(model => item.TokenID)
        </th>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Device.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Device.DeviceID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.TokenID }) |
            @Html.ActionLink("Details", "Details", new { id = item.TokenID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.TokenID })
        </td>
    </tr>
}

</table>
